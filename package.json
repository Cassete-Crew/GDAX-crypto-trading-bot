{
  "name": "express_boiler_plate",
  "type": "module",
  "version": "1.0.0",
  "description": "this is a boiler plate for building express applications in es6 js",
  "main": "app/server.js",
  "scripts": {
    "test": "jest",
    "dev:watch": "NODE_ENV=development nodemon --watch app --exec npm run start:dev",
    "start:dev": "npm run clean",
    "start": "NODE_ENV=production npm run serve",
    "clean": "rimraf build && mkdirp build && npm run build",
    "build": "babel app --out-dir build --ignore app/test/*.test.js && npm run serve",
    "serve": "node ./build/server.js"
  },
  "keywords": [
    "express",
    "boiler plate",
    "node"
  ],
  "engines": {
    "npm": "8.19.2",
    "node": "16.18.1"
  },
  "author": "callan brombacher",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "coinbase-pro": "github:coinbase/gdax-node",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "gdax": "^0.9.0",
    "request": "^2.88.2",
    "rimraf": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@types/node": "^18.15.0",
    "concurrently": "^7.6.0",
    "mkdirp": "^2.1.4",
    "nodemon": "^2.0.20"
  }
}
